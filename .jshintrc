{
  "node": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": true,
  // "curly": true,
  "eqeqeq": false,
  "eqnull": true,
  "immed": true,
  "indent": 4,
  "latedef": true,
  "newcap": false,
  "noarg": true,
  "predef": [
    // browser
    "document",
    "navigator",
    "prompt",
    "window",

    // plugins
    "$"
  ],
  // "quotmark": "double",
  "regexp": true,
  "undef": true,
  "unused": true,
  "strict": true,
  "trailing": true,
  "smarttabs": true,
  "white": true
}
